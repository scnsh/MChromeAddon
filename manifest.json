{
  "manifest_version": 2,
  "name": "CalGenPukiwiki",
  "description": "This extension generate monthy calendar by pukiwiki format.",
  "version": "1.0",
  "icons": {
    "16": "images/icon/icon_16.png",
    "48": "images/icon/icon_48.png",
    "128": "images/icon/icon_128.png"
  },  
  "browser_action": {
    "default_icon": {
        "19": "images/icon/icon_19.png",
        "38": "images/icon/icon_38.png"
    },
    "default_title": "CalGenPukiwiki",
    "default_popup": "popup.html"    
  },
//   "background": {
//     "scripts": ["js/jquery.js","js/background.js"]
//   },
  "permissions": [
    "tabs",
    "clipboardWrite"
  ]  
}