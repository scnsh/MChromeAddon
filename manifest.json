{
  "manifest_version": 2,
  "name": "PukiwikiCalendarGenerator",
  "description": "This extension generates working calendar with pukiwiki format.",
  "version": "1.1",
  "icons": {
    "16": "images/icon/icon_16.png",
    "48": "images/icon/icon_48.png",
    "128": "images/icon/icon_128.png"
  },  
  "browser_action": {
    "default_icon": {
        "19": "images/icon/icon_19.png",
        "38": "images/icon/icon_38.png"
    },
    "default_title": "PukiwikiCalendarGenerator",
    "default_popup": "popup.html"    
  },
  "options_page": "options.html",
  "permissions": [
    "clipboardWrite",
    "storage"
  ]  
}